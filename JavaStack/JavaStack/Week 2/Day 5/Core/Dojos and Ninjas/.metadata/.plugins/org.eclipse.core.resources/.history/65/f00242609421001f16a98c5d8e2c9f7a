import com.achref.dojoninja.repository.NinjaRepo;

@Service
public class NinjaSevice {
	
	@Autowired
	private NinjaRepo ninjaRepo;
	
	public Dojo createDojo(Dojo) {
		System.out.println(dojo);
		return dojo.save(dojo);
	}
}
